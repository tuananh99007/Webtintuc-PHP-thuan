function showFirebaseMsg(n){if(n!=undefined&&n.data!=undefined){if(!("Notification"in window)){console.log("This browser does not support desktop notification");return}var t=new Notification(n.data.title,n.data);t.onclick=function(i){if(i.preventDefault(),t.close(),n.data.tag!==""){if(getPathFromUrl(document.location.href)===getPathFromUrl(n.data.tag))return!0;window.open(n.data.tag,"_blank")}return!0}}}function getPathFromUrl(n){return n.split(/[?#]/)[0]}